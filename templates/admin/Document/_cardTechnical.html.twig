{% embed 'admin/_common/collapsible_card.twig' with { icon:'fa fa-gear', title: 'Document', width:12 } %}
    {% block card_body %}
        <div class="field-group">
            <div class="caption">ID</div>
            <div class="value">{{ document.id }}</div>
        </div>
        <div class="field-group">
            <div class="caption">Checksum</div>
            <div class="value">{{ document.checksum }}</div>
        </div>
        <div class="field-group">
            <div class="caption">Created</div>
            <div class="value">{{ document.created|date }}</div>
        </div>
    {% endblock %}
{% endembed %}
