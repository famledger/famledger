{% embed 'admin/_common/collapsible_card.twig' with { icon:'fa fa-code', title: 'Specs', width:12 } %}
    {% block card_body %}
        <div data-level="0" class="render-json">
            {% if document.specs|default(null) is not null %}
                {{ document.specs|json_encode|raw }}
            {% endif %}
        </div>
    {% endblock %}
{% endembed %}
